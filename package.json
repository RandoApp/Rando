{
  "name": "foodex",
  "version": "0.0.1",
  "description": "Server side of foodex project. Foodex - share your food with random people around the world",
  "engine": {
    "node": ">=0.8.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dimhold/foodex.git"
  },
  "dependencies": {
    "express": "3.x",
    "js-yaml": "3.0.x",
    "config": "0.4.x",
    "mongodb": "1.4.x",
    "mongoose": "3.8.x",
    "winston": "0.7.x",
    "async": "0.7.x",
    "validator": "2.1.x",
    "mv": "2.0.x",
    "mkdirp": "0.3.x",
    "gm": "1.14.x",
    "s3": "1.1.x"
  },
  "devDependencies": {
    "mocha": "*",
    "should": "*",
    "sinon": "*",
    "istanbul": "*",
    "coveralls": "*"
  },
  "scripts": {
    "test": "node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- --recursive",
    "coveralls": "node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha --report lcovonly -- --recursive && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
    }
}
